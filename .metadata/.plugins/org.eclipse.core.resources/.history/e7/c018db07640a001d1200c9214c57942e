package loop;

import java.util.Scanner;

public class Star01 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		
		for(int i = 1; i <= N; i++) {
			String str =  String.valueOf(i);
			str = "*";
			
			for(int a = 1; a <= N; a++) {
				System.out.println(a);
			}
		}

	}

}
