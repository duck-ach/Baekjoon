package array;

import java.util.Scanner;


public class ManipulateScore {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int C = sc.nextInt();
		int max = 0;
		int sum = 0;
		
		
		for(int i=0; i<C; i++) {
			// 점수 입력
			int score = sc.nextInt();
			int scores[] = new int[i];
			scores[i] = score;
			
			// 최대값 추출
			if(scores[i] > max) {
				max = scores[i];
			}
			
			// 점수합산
			for(int av=0; av<scores[scores.length]; i++) {
				sum += score;
			}
				
		}
		System.out.println(sum / max * 100);
		
	}

}
