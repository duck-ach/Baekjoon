package array;

import java.util.Scanner;

public class CountNumber {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int arr[] = new int[3];
		
		for(int i = 0; i < 3; i++) {
			int input = sc.nextInt();
			arr[i] = input;
			
			for(int j = 0; j < 2; j++) {
				arr[i] *= arr[i+1];
			}
		}

	}

}
