package loop;

import java.util.Scanner;

public class Star01 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		
		for(int i = 1; i <= N; i++) {
			for(int a = 1; a<=i; a++) {
			String str =  String.valueOf(i);
			str = "*";
			
			System.out.print(str);
			System.out.println();
			}
		}

	}

}
