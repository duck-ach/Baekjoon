package array;

import java.util.Scanner;


public class ManipulateScore {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int C = sc.nextInt();
		int Max = 0;
		
		
		for(int i=0; i<C; i++) {
			// 점수 입력
			int score = sc.nextInt();
			int scores[] = new int[i];
			scores[i] = score;
			
			// 점수 평
		}
		
	}

}
