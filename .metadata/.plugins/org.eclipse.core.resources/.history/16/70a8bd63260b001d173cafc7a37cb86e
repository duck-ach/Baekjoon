package array;

import java.util.Arrays;
import java.util.Scanner;


public class ManipulateScore {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int C = sc.nextInt();
		int[] arr = new int[C];
		int sum = 0;
		double avg = 0.0;
		
		for(int i=0; i<C; i++) {
			int score = sc.nextInt();
			arr[i] = score;
			
			sum += score;
		}
		
	}

}
