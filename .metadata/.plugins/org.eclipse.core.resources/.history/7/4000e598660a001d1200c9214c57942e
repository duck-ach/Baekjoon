package loop;

import java.util.Scanner;

public class Star02 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		
		for(int i = 1; i <= N; i++) {
			
			for(int a = N; a >= (N-1); a--) {
				System.out.print(" ");
			}
			for(int b = 1; b <= N; b++) {
				System.out.print("*");
			}
			
			System.out.print("\n");
		}

	}

}
